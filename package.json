{
  "name": "tsukimi",
  "version": "0.10.9",
  "description": "tsukimi media browser",
  "main": "public/index.html",
  "node-main": "tsukimi.js",
  "author": "Jacob Hipps <jacob@ycnrg.org>",
  "copyright": "Copyright (c) 2014-2016 J. Hipps/Neo-Retro Group, Inc.",
  "homepage": "https://tsukimi.io/",
  "license": "MPLv2",
  "nw_version": "0.15.0",
  "keywords": [
    "video",
    "browser",
    "player",
    "client",
    "scraper",
    "tv",
    "movies",
    "film",
    "anime"
  ],
  "scripts": {
    "start": "nw"
  },
  "user-agent": "%name/%ver (%nwver; %osinfo)",
  "no-edit-menu": true,
  "window": {
    "title": "tsukimi",
    "icon": "tsukimi_icon.png",
    "toolbar": false,
    "frame": true,
    "width": 1300,
    "height": 950,
    "min_width": 900,
    "min_height": 720,
    "position": "mouse"
  },
  "webkit": {
    "plugin": true
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/yellowcrescent/tsukimi"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "fs-xattr": "^0.1.14",
    "gulp": "^3.9.1",
    "gulp-bower": "0.0.13",
    "gulp-compass": "^2.1.0",
    "gulp-jshint": "^2.0.1",
    "gulp-jshint-bamboo": "0.0.2",
    "gulp-util": "^3.0.7",
    "jshint-stylish": "^2.2.0",
    "jshint-stylish-summary": "^0.1.2",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "mongodb": "^2.1.18",
    "request": "^2.72.0",
    "winston": "^2.2.0",
    "winston-mongodb": "^1.5.0",
    "xml2js": "^0.4.16"
  },
  "native_mods": [
    "fs-xattr"
  ]
}
