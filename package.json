{
  "name": "tsukimi",
  "version": "0.10.3",
  "description": "tsukimi media browser",
  "main": "public/index.html",
  "node-main": "tsukimi.js",
  "author": "Jacob Hipps <jacob@ycnrg.org>",
  "copyright": "Copyright (c) 2014-2016 J. Hipps/Neo-Retro Group, Inc.",
  "license": "MPLv2",
  "keywords": [
    "video",
    "browser",
    "player",
    "client",
    "scraper",
    "tv",
    "movies",
    "film",
    "anime"
  ],
  "scripts": {
    "start": "nw --enable-logging=stderr"
  },
  "user-agent": "%name/%ver (%nwver; %osinfo)",
  "no-edit-menu": true,
  "window": {
    "title": "tsukimi",
    "toolbar": false,
    "frame": true,
    "width": 1200,
    "height": 780,
    "min_width": 900,
    "min_height": 720,
    "position": "mouse"
  },
  "webkit": {
    "plugin": true
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/yellowcrescent/tsukimi"
  },
  "dependencies": {
    "mongodb": "^2.1.18"
  }
}
